{
  "name": "cordova-plugin-callkit",
  "version": "2.2.4",
  "description": "Cordova plugin that lets you use iOS CallKit UI (with PushKit) and Android ConnectionService UI",
  "cordova": {
    "id": "cordova-plugin-callkit",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattkhaw/cordova-plugin-callkit"
  },
  "scripts": {
    "build": "babel src/js --out-dir www"
  },
  "keywords": [
    "voip",
    "cordova",
    "push",
    "callkit",
    "pushkit",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "homepage": "https://github.com/mattkhaw/cordova-plugin-callkit",
  "author": "Matthew Khaw",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-header-comment": "^1.0.3"
  }
}
